{% extends '_template.html.twig' %}

{% block storePageLinkStyle %}
current_page
{% endblock %}
{% block main %}

    <h1>Store</h1>
    <hr>
    <table>
        <tr>
            <th>ID</th>
            <th>Description</th>
            <th>Price</th>

        </tr>
        <tr>
            {% for name in names %}
